    "def rotate_word(string,n):\n",
    "    letters = [] \n",
    "    for i in string:\n",
    "        letters.append(chr(ord(i)+n))\n",
    "    new_str = \"\".join(letters)    \n",
    "    return new_str"
   ]
  },
